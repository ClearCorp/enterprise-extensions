"\"{'group_by':'state'}\"/>\n"
"                    <filter string=\"Account Manager\" domain=\"[]\" context="
"\"{'group_by':'manager_id'}\"/>\n"
"                    <filter string=\"Partner\" domain=\"[]\" context="
"\"{'group_by':'partner_id'}\"/>\n"
"                    <filter string=\"Template\" domain=\"[]\" context="
"\"{'group_by':'template_id'}\"/>\n"
"                    <filter string=\"Start Month\" domain=\"[]\" context="
"\"{'group_by' : 'date_start'}\"/>\n"
"                    <filter string=\"End Month\" domain=\"[]\" context="
"\"{'group_by' : 'date'}\"/>\n"
"                    <filter string=\"Pricelist\" domain=\"[]\" context="
"\"{'group_by' : 'pricelist_id'}\"/>\n"
"                </group>\n"
"            </search>\n"
